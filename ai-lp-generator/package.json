{
  "name": "ai-lp-generator",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p ${PORT:-3001}",
    "dev:3001": "PORT=3001 next dev -p 3001",
    "dev:auto": "node scripts/dev-server.cjs",
    "build": "next build",
    "start": "next start -p ${PORT:-3001}",
    "start:3001": "PORT=3001 next start -p 3001",
    "start:prod": "NODE_ENV=production next start -p ${PORT:-3001}",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "generate": "node scripts/generate.js",
    "concept": "node scripts/concept.js",
    "ai-fix": "node scripts/ai-fix.js",
    "server:check": "node scripts/server-check.cjs",
    "server:kill": "node scripts/kill-port.cjs",
    "server:restart": "npm run server:kill && npm run dev:3001",
    "db:migrate": "node scripts/db-migrate.js",
    "health": "curl -f http://localhost:${PORT:-3001}/api/health || exit 1"
  },
  "dependencies": {
    "@ai-sdk/vercel": "^1.0.2",
    "@babel/parser": "^7.25.0",
    "@google/generative-ai": "^0.19.0",
    "@hello-pangea/dnd": "^18.0.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.50.0",
    "@types/archiver": "^6.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/prismjs": "^1.26.5",
    "ai": "^5.0.6",
    "archiver": "^7.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.400.0",
    "next": "^15.4.6",
    "node-fetch": "^3.3.2",
    "prism-react-renderer": "^2.4.1",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^4.1.8",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.4.6",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.5.0"
  }
}
